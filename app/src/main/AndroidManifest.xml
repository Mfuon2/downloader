<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ki.leo">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.SEND" />

                <category android:name="android.intent.category.DEFAULT" />

                <data android:mimeType="text/plain" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="io.fabric.ApiKey"
            android:value="b73e59c0a8ea3299fa76148739aeb97a79d1348a" />
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity android:name=".activities.AboutActivity" />

        <receiver
            android:name=".receivers.StopAutoListenReceiver"
            android:enabled="true" />

        <service
            android:name=".receivers.AutoListenService"
            android:exported="false" />

        <activity android:name=".activities.SampleDownloadActivity" />
        <activity android:name=".activities.DownloadYoutube"></activity>
    </application>

</manifest>